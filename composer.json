{
    "name": "swag/plugin-template",
    "description": "Template for Shopware 6 plugins",
    "version": "1.0.0",
    "type": "shopware-platform-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Shopware"
        }
    ],
    "require": {
        "shopware/core": "6.4.*@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "9.5.*",
        "league/flysystem-memory": "*",
        "symfony/phpunit-bridge": "*",
        "phpstan/phpstan": "0.12.90",
        "phpstan/phpstan-doctrine": "0.12.39",
        "phpstan/phpstan-phpunit": "0.12.20",
        "phpstan/phpstan-symfony": "0.12.37",
        "phpstan/extension-installer": "^1.1",
        "psalm/plugin-phpunit": "0.15.2",
        "psalm/plugin-symfony": "2.3.1",
        "vimeo/psalm": "4.8.1",
        "kubawerlos/php-cs-fixer-custom-fixers": "2.5.0",
        "symplify/easy-coding-standard": "9.3.15"
    },
    "extra": {
        "shopware-plugin-class": "Swag\\PluginTemplate\\SwagPluginTemplate",
        "copyright": "(c) by shopware AG",
        "label": {
            "de-DE": "SwagPluginTemplate",
            "en-GB": "SwagPluginTemplate"
        },
        "description": {
            "de-DE": "SwagPluginTemplate",
            "en-GB": "SwagPluginTemplate"
        }
    },
    "autoload": {
        "psr-4": {
            "Swag\\PluginTemplate\\": "src/"
        },
        "exclude-from-classmap": [
            "/tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Swag\\PluginTemplate\\Test\\": "tests/"
        }
    },
    "repositories": {
        "platform-packages": {
            "type": "path",
            "url": "../../../src/*"
        }
    }
}
